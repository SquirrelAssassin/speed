@{
    ViewBag.Title = "Azure Storage Latency Test";
}

<style>
    .x.axis line {
        shape-rendering: auto;
    }

    .line {
        fill: none;
        stroke-width: 3px;
    }
</style>
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <i class="fa fa-area-chart"></i>
                    Azure Storage Latency
                </div>
                <div class="chart-container">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <i class="fa fa-th-list"></i>
                    Latency Test
                </div>
                <table class="table table-bordered table-striped" id="latency-table">
                    <thead>
                        <tr>
                            <th>Geo</th>
                            <th>Region</th>
                            <th>Location</th>
                            <th>Average Latency (ms)</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>

@section scripts {
    <script type="text/javascript" src="~/Scripts/azurespeed/latency.js"></script>
}
